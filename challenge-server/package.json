{
  "name": "@blert/challenge-server",
  "version": "0.1.0",
  "description": "Server processing Blert challenges",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-path ../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../.prettierignore",
    "start": "node ./dist/app.js",
    "dev": "NODE_ENV=development tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/app.js\"",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.587.0",
    "@blert/common": "file:../common",
    "@types/redis": "^4.0.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.2",
    "postgres": "^3.4.4",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsc-watch": "^6.0.4"
  }
}
