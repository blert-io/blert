{
  "name": "@blert/common",
  "version": "0.1.0",
  "description": "Shared blert web and server code",
  "private": true,
  "license": "MIT",
  "scripts": {
    "generate-protos": "mkdir -p generated dist/generated && grpc_tools_node_protoc -I protos protos/*.proto --plugin=protoc_gen_ts --js_out=import_style=commonjs:generated --ts_out=generated",
    "build": "npm run generate-protos && tsc && cp generated/*.d.ts dist/generated",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-path ../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../.prettierignore",
    "migration:create": "tsc && node dist/migrations/migrate.js create $1",
    "migration:script": "tsc && node dist/migrations/migrate.js script $1",
    "migration:run": "tsc && node dist/migrations/migrate.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "generated/*.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.587.0",
    "postgres": "^3.4.4"
  },
  "overrides": {
    "@smithy/core": "3.20.0"
  }
}
